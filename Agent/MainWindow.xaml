<Window x:Class="Agent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agent"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="btExtractAllOffers" Content="Выгрузить все предложения из базы поставщикам" Click="btExtractAllOffers_Click"/>
        <Button Grid.Row="1" x:Name="btUploadNewOffers" Content="Загрузить данные выгрузок" Click="btUploadNewOffers_Click"/>
    </Grid>
</Window>
